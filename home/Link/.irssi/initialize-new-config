/alias crapclear scrollback LEVELCLEAR -level CLIENTCRAP,CLIENTNOTICES,CRAP ; lastlog -clear
/alias fix_prompt statusbar prompt reset;statusbar prompt add -after input -alignment right wpm;statusbar prompt add -before wpm -alignment right vim_mode;statusbar prompt add -before input -alignment left vim_windows;statusbar prompt position 100
/alias logtail clearable logtail $0
/bind ^Z insert_text !waifu
/bind ^[[Z word_completion_backward
